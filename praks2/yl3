#! /bin/bash
#
# kasutajate nimekiri kujul Perenimi, Eesnimi
#
# skripti algus
#
who | cut -f1 -d" " | cut -f2 -d"." > perenimi # l천ikame perenimi - see on 2.element peale punkti
who | cut -f1 -d" " | cut -f1 -d"." > eesnimi # l천ikame eesnimi - see on 1.element ennem punkti
paste -d"." perenimi eesnimi > nimed # kleebime need failid vastavas j채rjekorras kokku
awk {print} nimed | sort | uniq
#
# skripti l천pp

